{
    "name": "Blocker",
    "version": "1.0.0",
    "description": "Extension to block unsafe websites",
    "manifest_version": 3,
    "permissions": ["storage", "activeTab", "declartiveContent"],
    "author": "Ds Com",
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "action":{
        "default_popup": "index.html",
        "default_title": "Blocker"
    }
}